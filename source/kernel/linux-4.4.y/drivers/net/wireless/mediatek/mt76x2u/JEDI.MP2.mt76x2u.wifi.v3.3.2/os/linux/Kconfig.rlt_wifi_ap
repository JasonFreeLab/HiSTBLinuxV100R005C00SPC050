config RLT_AP_SUPPORT
	tristate "MediaTek RLT WiFi AP Support"
#	depends on NET_RADIO
	select WIRELESS_EXT
	select WEXT_SPY
	select WEXT_PRIV

config WDS_SUPPORT
	bool "WDS"
	depends on RLT_AP_SUPPORT
	default y

config MBSS_SUPPORT
	bool "MBSSID"
	depends on RLT_AP_SUPPORT
config SNIFFER_SUPPORT
	bool "SNIFFER"
	depends on RLT_AP_SUPPORT
	default n

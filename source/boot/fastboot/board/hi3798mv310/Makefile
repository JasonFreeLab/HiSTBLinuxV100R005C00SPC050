
include $(TOPDIR)/config.mk

LIB	= $(obj)lib$(BOARD).a

COBJS	:= board.o eth_cfg_setup.o chiptrim.o

SRCS	:= $(COBJS:.o=.c)
OBJS	:= $(addprefix $(obj),$(COBJS))

$(LIB):	$(obj).depend $(OBJS)
	$(AR) $(ARFLAGS) $@ $(OBJS)

clean:
	rm -f $(OBJS)

distclean:	clean
	rm -f $(LIB) core *.bak $(obj).depend

#########################################################################

# defines $(obj).depend target
include $(SRCTREE)/rules.mk

#########################################################################
sinclude $(obj).depend
